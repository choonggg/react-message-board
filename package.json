{
  "private": true,
  "name": "front-end-message-board",
  "sideEffects": false,
  "babel": {
    "extends": "./config/.babelrc"
  },
  "scripts": {
    "analyze": "webpack --config config/webpack.analyze.js",
    "api": "json-server -d 800 -p 3001 config/api.js",
    "build": "webpack --config config/webpack.prod.js",
    "develop": "webpack-dev-server --config config/webpack.dev.js",
    "start": "concurrently --kill-others -n API,DEV -c green,cyan  \"npm run api\" \"npm run develop\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.0",
    "css-loader": "^0.28.11",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "json-server": "^0.12.2",
    "react-hot-loader": "^4.3.3",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.15.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  }
}
